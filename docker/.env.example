# Docker Compose Environment Variables
#
# Copy this file to .env and customize as needed:
#   cp .env.example .env
#

# MySQL Configuration
MYSQL_ROOT_PASSWORD=regluit_root_pass
MYSQL_DATABASE=regluit
MYSQL_USER=regluit
MYSQL_PASSWORD=regluit_pass
MYSQL_PORT=3306

# Redis Configuration
REDIS_PORT=6379

# Optional: Adminer (database UI)
ADMINER_PORT=8080

# Optional: Redis Commander
REDIS_COMMANDER_PORT=8081

# Optional: MailHog (email testing)
MAILHOG_SMTP_PORT=1025
MAILHOG_WEB_PORT=8025
